{
    "editor.fontSize": 12,
    "editor.fontWeight": "500",
    "terminal.integrated.fontSize": 14,
    "window.nativeTabs": true,
    "explorer.confirmDelete": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.suggestSelection": "first",
    "terminal.external.osxExec": "iTerm2.app",
    "workbench.colorTheme": "Gruvbox Material Dark",
    "editor.fontFamily": "Hack Nerd Font, Monaco, 'Courier New', monospace",
    "window.zoomLevel": 2,
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": [
        "#8FBCBB",
        "#434C5E"
    ],
  "files.exclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/tmp": true,
    "**/bower_components": true,
    "assets/**": true,
    "backup/**": true,
    "cache/**": true,
    "images/**": true,
    "logs/**": true,
    "system/**": true,
    "tmp/**": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/tmp": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "assets/**": true,
    "backup/**": true,
    "bin/**": true,
    "cache/**": true,
    "images/**": true,
    "logs/**": true,
    "system/**": true,
    "tmp/**": true,
    "build/**": true
  },
    "vim.statusBarColors.insert": "#2c3e50",
    "vim.statusBarColors.visual": "#e74c3c",
    "vim.statusBarColors.visualline": "#e74c3c",
    "vim.statusBarColors.visualblock": "#A3BE8C",
    "vim.statusBarColors.replace": "#D08770",
    "vim.statusBarColors.commandlineinprogress": "#007ACC",
    "vim.statusBarColors.searchinprogressmode": "#007ACC",
    "vim.statusBarColors.easymotionmode": "#007ACC",
    "vim.statusBarColors.easymotioninputmode": "#007ACC",
    "vim.statusBarColors.surroundinputmode": "#007ACC",
    "workbench.colorCustomizations": {
      "statusBar.background": "#2c3e50",
      "statusBar.noFolderBackground": "#2c3e50",
      "statusBar.debuggingBackground": "#2c3e50",
      "statusBar.foreground": "#434C5E"
    },
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.dragAndDrop": false,
    "editor.lineNumbers": "relative",
    "go.formatTool": "goimports",
    "editor.minimap.enabled": false,
    "window.newWindowDimensions": "fullscreen",
    "window.restoreFullscreen": true
}
